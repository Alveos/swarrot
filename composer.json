{
    "name":        "swarrot/swarrot",
    "description": "A simple lib to consume RabbitMQ queues",
    "keywords": ["swarrot","worker"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Olivier Dolbeau",
            "homepage": "http://odolbeau.fr/"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/options-resolver": "~2.4",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "phpspec/prophecy": "~1.1",
        "videlalvaro/php-amqplib": "~2.0"
    },
    "replace": {
        "swarrot/ack-processor": "self.version",
        "swarrot/exception-catcher-processor": "self.version",
        "swarrot/instant-retry-processor": "self.version",
        "swarrot/max-messages-processor": "self.version",
        "swarrot/max-execution-time-processor": "self.version",
        "swarrot/signal-handler-processor": "self.version"
    },
    "suggest": {
        "pecl-amqp": "*",
        "videlalvaro/php-amqplib": "~2.0",
        "symfony/console": "~2.4"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "Swarrot": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
