{
    "name":        "swarrot/swarrot",
    "description": "A simple lib to consume RabbitMQ queues",
    "keywords": ["swarrot","worker"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Olivier Dolbeau",
            "homepage": "http://odolbeau.fr/"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/options-resolver": "~2.4"
    },
    "require-dev": {
        "phpspec/phpspec": "2.0.*@dev"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "Swarrot": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
